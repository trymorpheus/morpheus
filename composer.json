{
    "name": "dynamiccrud/dynamiccrud",
    "description": "Librería PHP para generar formularios CRUD dinámicos con validación automática basada en estructura de base de datos",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "crud",
        "forms",
        "validation",
        "dynamic",
        "database"
    ],
    "authors": [
        {
            "name": "Mario Raúl Carbonell Martínez",
            "email": "marioraulcarbonell@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "DynamicCRUD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DynamicCRUD\\Tests\\": "tests/"
        }
    }
}